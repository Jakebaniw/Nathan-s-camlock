local state = true
TextButton.MouseButton1Click:Connect(
    function()
        state = not state
        if not state then
            TextButton.Text = "Navry's Cam On"
            CamlockState = true
            enemy = FindNearestEnemy()
        else
            TextButton.Text = "Navry's Cam Off"
            CamlockState = false
            enemy = nil
        end
    end
)

UICorner_2.Parent = TextButton


